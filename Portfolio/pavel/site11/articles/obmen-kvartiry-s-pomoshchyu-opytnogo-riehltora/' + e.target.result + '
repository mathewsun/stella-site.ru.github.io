	<script>
		$(document).on('change', 'input[name="PROPERTY[22]"]', function () {
			var ans = $(this).attr('id');
			if (ans == "property_1") {
				$('.forYesAns').addClass('active');
				$('.forNoAns').removeClass('active');
			}
			if (ans == "property_2") {
				$('.forYesAns').removeClass('active')
				$('.forNoAns').addClass('active')
			}
		});
		$(document).on('click', '.js-add-photo', function (e) {
			e.preventDefault();
			$('#img').click();
		});
		$(document).on('change', '#img', function (e) {
			e.preventDefault();
			var input = $(this)[0];
			if (input.files && input.files[0]) {
				if (input.files[0].type.match('image.*')) {
					var reader = new FileReader();
					reader.onload = function (e) {
						var result = $(input).attr('name').split('_');
						var new_count_name = result[0] + '[' + result[2] + ']' + '[' + result[3] + ']';
						result[3]++;
						var new_name = result.join('_');
						var new_input = $(input).clone();
						$(new_input).attr('name', new_name).attr('id', 'img');
						$(input).removeAttr('id');
						var add_but = $('.js-add-photo');
						add_but.remove();
						$('ul.pic-list').append('<li class="pic-list-item"><div class="pic-list-wrap"><img src="' + e.target.result + '"/></div></li>');
						$('ul.pic-list').append('<li class="pic-list-item js-add-photo"><a href="#" class="add-photo-btn">Добавить фото</a></li>');
						$('ul.pic-list').prepend('<input type="hidden" name ="' + new_count_name + '" />');
						$('ul.pic-list').prepend($(new_input));
					};
					reader.readAsDataURL(input.files[0]);
					$(input).removeAttr('id');

				} else {
					console.log('ошибка, не изображение');
				}
			} else {
				console.log('хьюстон у нас проблема');
			}
		});
		$(document).on('click','.input-variants li', function(e){
			e.preventDefault();
			var choose = $(this).data('value');
			$(this).parents('.input-wrap').find('.js-controlled-select').val(choose);
		})
	</script>
